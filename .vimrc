set encoding=utf-8
scriptencoding utf-8
set nocompatible
set smartcase
set showcmd
set wildmenu
set wildmode=longest,full
set nowb
set nobackup
set noswapfile
set number
set laststatus=2
set statusline=
set statusline+=%F\ 
set statusline+=%=
set statusline+=%m\ 
set statusline+=%l:%c
set title
set titlestring=%t
set titleold=
set tabstop=4
set shiftwidth=4
set softtabstop=4
filetype indent plugin on
syntax on
set autoindent
set number relativenumber
set nu rnu
nnoremap j k
nnoremap k j
